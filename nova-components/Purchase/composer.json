{
    "name": "augusto/purchase",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Augusto\\Purchase\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Augusto\\Purchase\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
